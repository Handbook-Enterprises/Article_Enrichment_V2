# API Keys
OPENROUTER_API_KEY=
OPENAI_API_KEY= 

# LLM Configuration
OPENROUTER_MODEL=openai/gpt-5-mini
LLM_DEBUG=1
LLM_TIMEOUT=60
LLM_TEMPERATURE=0.2

# Content Processing
ANCHOR_SANITIZE=0
ANCHOR_REQUIRE_KEYWORDS=0
PROMPT_MODE=both

# QA Configuration
QA_MODE=ai
QA_THRESHOLD=6

# Pipeline Configuration
RETRY_MAX_ATTEMPTS=2
USE_CREWAI_AGENTS=0

# Flow Persistence (Redis)
USE_FLOW=1
FLOW_REDIS_URL=redis://redis:6379/2

# Celery (for FastAPI async processing)
CELERY_BROKER_URL=redis://redis:6379/0
CELERY_RESULT_BACKEND=redis://redis:6379/1